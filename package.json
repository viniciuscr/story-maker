{
  "name": "story-maker-turbo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "manypkg fix"
  },
  "dependencies": {
    "@manypkg/cli": "latest",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "1.10.14"
  },
  "packageManager": "bun@1.0.3"
}
